{#
/**
 * @file
 * Theme override to display a region.
 *
 * Available variables:
 * - content: The content for this region, typically blocks.
 * - attributes: HTML attributes for the region <div>.
 * - region: The name of the region variable as defined in the theme's
 *   .info.yml file.
 *
 * @see template_preprocess_region()
 */
#}
{%
    set classes = [
      'region',
      'region-' ~ region|clean_class,
    ]
  %}
  {% if content %}
    <div{{ attributes.addClass(classes) }}>
      {{ content }}

      <div class="social-media-links">
            <a href="https://github.com/visuale" target="_new" title="Visuale on Github" class="visuale-github">
                {% include '@visuale/../src/images/visuale-github.svg' %}
            </a>
            <a href="https://twitter.com/visualechicago" target="_new" title="Visuale on Twitter" class="visuale-twitter">
                {% include '@visuale/../src/images/visuale-twitter.svg' %}
            </a>
      </div>
    </div>
  {% endif %}
  